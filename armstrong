#include <stdio.h>
#include <math.h>
int main()
{
    int n, sum=0, r, num, c=0, num1;
    printf("Enter the number you want to check:");
    scanf("%d",&n);
    num=n;
    while(n>0)
    {
    n=n/10;
    c++;
    }
    num1=num;
    
    while(num>0)
    {
        r=num%10;
        sum=sum+pow(r,c);
        num=num/10;
    }
    if(sum==num1)
    printf("The number is an armstrong number.");
    else
    printf("The number is not an armstrong number.");
    return 0;
}
